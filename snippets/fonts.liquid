{% style %}
  @font-face {
    font-family: "ABCOracle";
    font-style: normal;
    font-weight: 400;

    font-display: fallback;
    src: url('{{ 'ABCOracle-Regular.woff2' | asset_url }}') format('woff2'), url('{{ 'ABCOracle-Regular.woff' | asset_url
    }}') format('woff');
  }

  @font-face {
    font-family: "ABCOracle";
    font-style: italic;
    font-weight: 400;

    font-display: fallback;
    src: url('{{ 'ABCOracle-RegularItalic.woff2' | asset_url }}') format('woff2'), url('{{ 'ABCOracle-RegularItalic.woff' |
      asset_url }}') format('woff');
    }

    @font-face {
      font-family: "ABCOracle";
      font-style: normal;
      font-weight: 700;

      font-display: fallback;
      src: url('{{ 'ABCOracle-Bold.woff2' | asset_url }}') format('woff2'), url('{{ 'ABCOracle-Bold.woff' | asset_url }}')
      format('woff');
    }

